{
  "name": "passenjr",
  "version": "1.0.0",
  "description": "Passenjr is a journey planner application",
  "main": "server.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config ./webpack.prod.config.js",
    "lint": "eslint scripts",
    "start": "node ./server/server.js",
    "start-production": "NODE_ENV=production node ./server/server.js",
    "test": "mocha --compilers js:babel-core/register --recursive -r mock-local-storage",
    "prebuild": "sed -i.bak \"s/'.*'/'${PASSENJR_PROD:-f4323c6f7c0cd73d2d786a2b1cdae80c}'/\" ./scripts/constants/Config.js",
    "postbuild": "sed -i.bak \"s/'.*'/'${PASSENJR_DEV:-f4323c6f7c0cd73d2d786a2b1cdae80c}'/\" ./scripts/constants/Config.js && rm ./scripts/constants/Config.js.bak"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lutando/Passenjr.git"
  },
  "author": "Lutando Ngqakaza",
  "license": "GNU GPL",
  "bugs": {
    "url": "https://github.com/Lutando/Passenjr/issues"
  },
  "homepage": "https://github.com/Lutando/Passenjr",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.12.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fastclick": "^1.0.6",
    "isomorphic-fetch": "^2.1.1",
    "jquery": "^3.1.1",
    "jsx-loader": "^0.13.2",
    "material-ui": "^0.16.0",
    "mocha": "^2.3.3",
    "mock-local-storage": "^1.0.2",
    "nock": "^8.0.0",
    "node-sass": "^3.3.3",
    "normalizr": "^2.0.0",
    "postcss-loader": "^0.13.0",
    "react-hot-loader": "^1.3.0",
    "remote-redux-devtools": "^0.5.0",
    "sass-loader": "^3.2.0",
