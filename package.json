    "classnames": "^2.2.5",
    "connect-timeout": "^1.7.0",
    "express": "^4.13.3",
    "js-cookie": "^2.1.3",
    "leaflet": "^1.0.1",
    "lodash": "^4.15.0",
    "moment": "^2.15.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-leaflet": "^0.12.3",
    "react-motion": "^0.4.4",
    "react-redux": "^4.4.5",
